<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/DylanCoordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightBlue"
    tools:context=".DylanActivity">

    <TextView
        android:id="@+id/DylanTextView"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:text="@string/welcome_n_dylan_ashton_n01442206" />

    <com.google.android.material.textfield.TextInputEditText
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:id="@+id/DylanEditText"
        android:hint="@string/type_here"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="30dp" />

    <Button
        android:id="@+id/DylanButton1"
        android:layout_width="281dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp"
        android:layout_marginStart="70dp"
        android:text="@string/button_1"
        android:textSize="42dp" />

    <Button
        android:id="@+id/DylanButton2"
        android:layout_width="281dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="300dp"
        android:text="@string/button_2"
        android:textSize="42dp" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>
